{
  "expo": {
    "name": "CortoCrudo",
    "slug": "CortoCrudo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/corto-crudo-logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "CortoCrudo",
    "splash": {
      "image": "./assets/corto-crudo-logo.png",
      "resizeMode": "cover"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.CortoCrudo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/corto-crudo-logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.CortoCrudo.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}