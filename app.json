{
  "expo": {
    "name": "CortoCrudo",
    "slug": "CortoCrudo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/corto-crudo-logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "scheme": "cortocrudo",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.CortoCrudo.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.CortoCrudo.app"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-video",
      [
        "expo-image-picker",
        {
          "photosPermission": "CortoCrudo necesita acceso a tus fotos para actualizar tu foto de perfil.",
          "cameraPermission": "CortoCrudo necesita acceso a la c√°mara para tomar una foto de perfil."
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "eb80a16a-3fe1-4d35-a79e-0959ce0966c0"
      }
    }
  }
}
